{
    "name": "artishup/prime",
    "description": "Hexagonal Architecture Skeleton",
    "keywords": ["artishup", "hexagonal", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "aws/aws-php-sns-message-validator": "^1.6",
        "aws/aws-sdk-php-laravel": "^3.4",
        "lambdish/phunctional": "^2.0",
        "laravel/lumen-framework": "^6.0",
        "ramsey/uuid": "^3.9"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "ArtishUp\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
